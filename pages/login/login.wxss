.registerContainer {
  background-color: #fbfbfb;
  display: flex;
  flex-direction: column;
  height: 100%;
}

.phone {
  display: flex;
  flex-direction: row;
  margin-left: 130rpx;
  margin-top: 350rpx;
}

.inputPhone {
  border-bottom: 1rpx solid #d5dddf;
  margin-left: 20rpx;
  padding: 5rpx;
  font-size: 30rpx;
  width: 380rpx;
}

.accessNum {
  display: flex;
  flex-direction: row;
  margin-left: 110rpx;
  margin-top: 20rpx;
  padding: 20rpx;
}

.get {
  position: absolute;
  background: #0068aa;
  font-size: 30rpx;
  color: #fff;
  text-align: center;
  padding: 10rpx 35rpx;
  border-radius: 5rpx;
  margin-left: 340rpx;
  margin-top: 10rpx;
}

.get02 {
  position: absolute;
  background: red;
  font-size: 30rpx;
  color: #fff;
  text-align: center;
  padding: 10rpx 35rpx;
  border-radius: 5rpx;
  margin-left: 340rpx;
  margin-top: 10rpx;
}

.inputAccess {
  margin-top: 20rpx;
  width: 250rpx;
}

.register {
  text-align: center;
  border-radius: 5rpx;
  padding: 15rpx;
  margin: 80rpx 150rpx 10rpx;
  background: #0068aa;
  font-size: 40rpx;
  color: #fff;
}

.login {
  margin: 10rpx 140rpx;
  font-size: 30rpx;
  color: #0068aa;
  font-weight: 390;
  padding: 5rpx 15rpx;
  border-radius: 5rpx;
  text-align: right;
}
